{
    "appenders": {
        "out": {
            "type": "stdout",
            "layout": {
                "type": "pattern",
                "pattern": "%[%d %p %c %f{2}: %m%]"
            }
        }
    },
    "categories": {
        "default": {
            "appenders": ["out"],
            "level": "debug",
            "enableCallStack": true
        }
    }
}
